                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT dkbldqt.MaSV, [user].HoTen, dkbldqt.HoTenSV, dkbldqt.MaLop, dkbldqt.NgaySinh, CONVERT(MONEY, REPLACE(CONVERT(VARCHAR, CONVERT(MONEY, 
                  dkbldqt.TongSoTC * HocKy.HocPhi), 1), '.00', '')) AS TienHP, RIGHT(HocKy.HocKy, 1) AS Hocky, CONVERT(int, SUBSTRING(HocKy.HocKy, 0, 5)) + 1 AS NH, 
                  SUBSTRING(HocKy.HocKy, 0, 5) AS NamHoc, DATEPART(yy, GETDATE()) AS Nam, DATEPART(DD, GETDATE()) AS Ngay, DATEPART(MM, GETDATE()) AS Thang
FROM     [user] CROSS JOIN
                  dkbldqt INNER JOIN
                  HocKy ON dkbldqt.HocKyID = HocKy.HocKyID
WHERE  (dkbldqt.NgayDong IS NULL) AND (HocKy.HocKy = @HocKy) AND ([user].HoTen = @HT)
ORDER BY dkbldqt.MaLop, dkbldqt.NgayDong</CommandText>
