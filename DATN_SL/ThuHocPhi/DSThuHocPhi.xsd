                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT dkbldqt.MaSV, [user].HoTen, dkbldqt.HoTenSV, dkbldqt.MaLop, dkbldqt.NgaySinh, REPLACE(CONVERT(VARCHAR, CONVERT(MONEY, dkbldqt.TongSoTC * HocKy.HocPhi), 1), 
                  '.00', '') AS TienHP, RIGHT(HocKy.HocKy, 1) AS Hocky, CONVERT(int, SUBSTRING(HocKy.HocKy, 0, 5)) + 1 AS NH, SUBSTRING(HocKy.HocKy, 0, 5) AS NamHoc, 
                  dbo.Num2Text(dkbldqt.TongSoTC * HocKy.HocPhi) AS TienHP_Text, HocKy.TrangThai, DATEPART(yy, GETDATE()) AS Nam, DATEPART(DD, GETDATE()) AS Ngay, 
                  DATEPART(MM, GETDATE()) AS Thang
FROM     [user] INNER JOIN
                  dkbldqt INNER JOIN
                  HocKy ON dkbldqt.HocKyID = HocKy.HocKyID ON [user].username = dkbldqt.NguoiThu
WHERE  (dkbldqt.MaSV = @MaSV)
ORDER BY dkbldqt.MaLop</CommandText>
                    <Parameters>
